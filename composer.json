{
  "name": "jfhovinne/updater",
  "type": "drupal-drush",
  "require": {
    "danielstjules/stringy": "~3.1.0"
  },
  "require-dev": {
    "drupal/coder": "8.2.8",
    "drupal/core": "~8",
    "drush/drush": "~8.0",
    "nuvoleweb/drupal-component-scaffold": "*",
    "phpro/grumphp": "*",
    "squizlabs/php_codesniffer": "~2.9"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/jfhovinne/drupal-component-scaffold"
    }
  ],
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/sites/all/drush/{$name}": ["type:drupal-drush"],
      "web/modules/contrib/{$name}": ["type:drupal-module"]
    }
  },
  "autoload": {
    "psr-4": {
      "Drush\\updater\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
